<feature
      id="org.haxe4e.studio.feature"
      label="%name"
      version="1.0.0.qualifier"
      provider-name="The Haxe4E Project"
      plugin="org.haxe4e.studio.plugin"
      license-feature="org.eclipse.license"
      license-feature-version="2.0.2.qualifier">

   <description url="https://github.com/haxe4e/haxe-studio/">
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="Haxe4E Latest" url="https://raw.githubusercontent.com/haxe4e/haxe-studio/updatesite/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.contenttype"/>
      <import plugin="org.eclipse.core.externaltools"/>
      <import plugin="org.eclipse.core.filebuffers"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.debug.core"/>
      <import plugin="org.eclipse.debug.ui"/>
      <import plugin="org.eclipse.jface"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.osgi"/>
      <import plugin="org.eclipse.tm4e.registry"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.ui.externaltools"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui.ide.application"/>
      <import plugin="org.eclipse.ui.genericeditor"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      <import plugin="de.sebthom.eclipse.commons"/>
      <import plugin="net.sf.jstuff.core" version="5.18.0" match="greaterOrEqual"/>
      <import plugin="org.apache.commons.commons-io" version="2.11.0" match="greaterOrEqual"/>
      <import plugin="org.apache.commons.lang3" version="3.12.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.haxe4e.studio.plugin"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="de.sebthom.eclipse.commons"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="net.sf.jstuff.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.apache.commons.lang3"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.apache.commons.commons-io"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
